{
  "name": "daminhgovap",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:compile": "next build --experimental-build-mode compile",
    "build:generate-env": "next build --experimental-build-mode generate-env",
    "build:generate": "next build --experimental-build-mode generate",
    "start": "next start",
    "prestandalone": "pnpm run payload migrate && pnpm run build:generate-env && pnpm run build:generate && cp -r public .next/standalone/ && cp -r .next/static .next/standalone/.next/",
    "standalone": "node ./.next/standalone/server.js",
    "check-types": "tsc --noemit",
    "prepare": "husky",
    "lint": "eslint .",
    "lint:fix": "(eslint --fix . && npm run prettier:fix) || npm run prettier:fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate": "pnpm run generate:importmap && pnpm run generate:types",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "@payloadcms/db-mongodb": "~3.56.0",
    "@payloadcms/live-preview-react": "~3.56.0",
    "@payloadcms/next": "~3.56.0",
    "@payloadcms/payload-cloud": "~3.56.0",
    "@payloadcms/richtext-lexical": "~3.56.0",
    "@payloadcms/translations": "~3.56.0",
    "@payloadcms/ui": "~3.56.0",
    "@preline/carousel": "^3.2.3",
    "@preline/dropdown": "^3.2.3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tanstack/react-query": "^5.71.5",
    "@uiw/react-md-editor": "^4.0.5",
    "dayjs": "^1.11.13",
    "firebase": "^12.4.0",
    "fuse.js": "^7.1.0",
    "graphql": "^16.10.0",
    "gray-matter": "^4.0.3",
    "next": "15.4.6",
    "next-intl": "^4.3.9",
    "normalize-text": "^2.6.0",
    "payload": "~3.56.0",
    "preline": "^3.2.3",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.2",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-react": "^7.26.3",
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "15.5.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/eslint-plugin-query": "^5.71.5",
    "@types/node": "^20.19.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.5.2",
    "frontmatter-markdown-loader": "^3.7.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@firebase/util",
      "protobufjs",
      "sharp"
    ],
    "overrides": {
      "@types/react": "19.1.12",
      "@types/react-dom": "19.1.8"
    }
  },
  "engines": {
    "npm": "please use pnpm",
    "yarn": "please use pnpm",
    "pnpm": ">= 10.13.1"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
